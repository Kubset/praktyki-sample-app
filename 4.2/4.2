class Mapa:
    def __init__(self):
  
        self.mapa = [
            "#########################",
            "#.......................#",
            "#.......................#",
            "#.......................#",
            "#.......................#",
            "#.......................#",
            "#.......................#",
            "#.......................#",
            "#.......................#",
            "#.......................#",
            "#.......................#",
            "#########################",
        ]
        self.szerokosc = len(self.mapa[0])
        self.wysokosc = len(self.mapa)

      
        self.dane = [[1 if znak == '#' else 0 for znak in linia] for linia in self.mapa]

    def czy_dostepne(self, x, y):
      
        return self.dane[y][x] == 0